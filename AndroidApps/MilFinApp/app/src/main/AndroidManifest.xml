        <activity
            android:name=".Savings"
            android:label="savingsActivity">
        </activity>
        <activity
            android:name=".Tips"
            android:label="tipsActivity">
        </activity>
        <activity
            android:name=".Portfolio"
            android:label="portfolioActivity">
        </activity>
    </application>

</manifest>
